{
  "name": "issue-api",
  "version": "1.0.0",
  "description": "Issue API with migrations, seeders, Docker, and swagger",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-jwt": "^4.0.4",
    "koa-router": "^12.0.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.9.7",
    "proxyquire": "^2.1.3",
    "sequelize": "^6.37.3"
  },
  "author": "Testlio",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-extended": "^5.0.3",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "sequelize-test-helpers": "^1.4.3",
    "sinon": "^20.0.0",
    "supertest": "^7.1.1"
  }
}
